{
  "name": "common-express",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bunx prisma generate && bun src/index.ts",
    "dev": "bun --watch src/index.ts",
    "dev:docker": "bunx prisma generate && bun --watch src/index.ts",
    "migrate": "bunx prisma migrate dev --name init",
    "db:generate": "bunx prisma generate --schema=./src/database/prisma/schema.prisma"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "prisma": "^6.17.1"
  },
  "peerDependencies": {
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@prisma/client": "^6.17.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "chalk": "^5.6.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "path": "^0.12.7",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "zod": "^3.25.76"
  },
  "prisma": {
    "schema": "src/database/prisma/schema.prisma"
  }
}